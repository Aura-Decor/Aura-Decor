<configuration>
    <system.webServer>
        <handlers>
            <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified"/>
        </handlers>
        <aspNetCore processPath="dotnet" arguments="AuraDecor.APIs" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
        <defaultDocument enabled="true">
            <files>
                <add value="swagger/index.html"/>
                <add value="default.aspx"/>
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
